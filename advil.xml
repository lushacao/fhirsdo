              <code code="55105-1" codeSystem="2.16.840.1.113883.6.1" displayName="OTC - PURPOSE SECTION"/>
              <text>
                <table width="60%">
                  <col align="left" valign="bottom" width="50%"/>
                  <col align="right" valign="bottom" width="50%"/>
                  <thead>
                    <tr>
                      <th>
                        <content styleCode="italics">Active ingredients<br/>(in each tablet)</content>
                      </th>
                      <th>
                        <content styleCode="italics">Purposes</content>
                      </th>
                    </tr>
                  </thead>
                  <tbody>
                    <tr>
                      <td>Chlorpheniramine maleate 4 mg</td>
                      <td>Antihistamine</td>
                    </tr>
                    <tr>
                      <td>Ibuprofen 200 mg (NSAID)<footnote>nonsteroidal anti-inflammatory drug</footnote>
                      </td>
                      <td>Pain reliever/Fever reducer</td>
                    </tr>
                    <tr>
                      <td>Phenylephrine HCl 10 mg</td>
                      <td>Nasal decongestant</td>
                    </tr>
                  </tbody>
                </table>
